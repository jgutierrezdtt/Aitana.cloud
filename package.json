{
  "name": "vulnerable_web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "dev:development": "NODE_ENV=development next dev --turbopack",
    "dev:production": "NODE_ENV=production next dev --turbopack",
    "build": "next build",
    "build:development": "NODE_ENV=development next build",
    "build:production": "NODE_ENV=production next build",
    "build:turbo": "next build --turbopack",
    "start": "next start",
    "start:development": "NODE_ENV=development next start",
    "start:production": "NODE_ENV=production next start",
    "lint": "eslint",
    "test:theme": "node scripts/verify-theme.js",
    "test:dark": "node scripts/test-dark-elements.js",
    "test:all": "npm run test:theme && npm run test:dark",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:seed": "tsx prisma/seed.ts",
    "db:reset": "prisma db push --force-reset && npm run db:seed",
    "db:studio": "prisma studio",
    "setup:local": "npm run db:generate && npm run db:push && npm run db:seed",
    "setup:production": "npm run db:generate && npm run db:push && npm run db:seed",
    "vercel-build": "npm run db:generate && next build",
    "deploy:preview": "vercel --env NEXT_PUBLIC_ENV=development",
    "deploy:production": "vercel --prod --env NEXT_PUBLIC_ENV=production"
  },
  "dependencies": {
    "@neondatabase/serverless": "^1.0.1",
    "@prisma/client": "^6.19.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/js-cookie": "^3.0.6",
    "@types/js-yaml": "^4.0.9",
    "@types/jsonwebtoken": "^9.0.10",
    "bcryptjs": "^3.0.2",
    "framer-motion": "^12.23.26",
    "js-cookie": "^3.0.5",
    "js-yaml": "^4.1.0",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.556.0",
    "next": "^16.0.7",
    "next-intl": "^4.7.0",
    "prisma": "^6.19.0",
    "react": "^19.2.1",
    "react-dom": "^19.2.1",
    "sqlite3": "^5.1.7"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^24.10.1",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "^16.0.7",
    "tailwindcss": "^4",
    "tsx": "^4.21.0",
    "typescript": "^5"
  }
}
